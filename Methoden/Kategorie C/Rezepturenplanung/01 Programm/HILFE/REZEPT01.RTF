\par }\pard\plain \s22\nowidctlpar \b\f5\fs28\cf2\lang1031 \page {\cs19\super #{\footnote \pard\plain \s16\widctlpar \f4\fs20\lang1031 {\cs19\super #} INIDATEI}${\footnote \pard\plain \s16\widctlpar \f4\fs20\lang1031 {\cs19\super $} Die INI-Datei}}{
