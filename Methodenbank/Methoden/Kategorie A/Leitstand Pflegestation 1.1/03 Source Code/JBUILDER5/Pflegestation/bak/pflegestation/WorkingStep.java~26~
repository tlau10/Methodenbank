package pflegestation;

/**
 * <p>Title: Leitstand Pflegestation</p>
 * <p>Description: Optimiert die Pflegevorgänge einer Krankenhausstation</p>
 * <p>Copyright: Copyright (c) 2002</p>
 * <p>Company: FH Konstanz</p>
 * @author Sebastian Hagen, Jonathan Feuerstein, Birgit Engler
 * @version 1.0
 */

public class WorkingStep
{
    private String m_strRegularStartHour;
    private String m_strRegularStartMinute;
    private String m_strRegularEndHour;
    private String m_strRegularEndMinute;
    private String m_strAlternativeStartHour;
    private String m_strAlternativeStartMinute;
    private String m_strAlternativeEndHour;
    private String m_strAlternativeEndMinute;
    private String m_strDuration;
    private int m_intNumber;
    private String m_strDescription;

    public WorkingStep( int in_intNumber )
    {
        m_strRegularStartHour = "00";
        m_strRegularStartMinute = "00";
        m_strRegularEndHour = "00";
        m_strRegularEndMinute = "00";
        m_strAlternativeStartHour = "00";
        m_strAlternativeStartMinute = "00";
        m_strAlternativeEndHour = "00";
        m_strAlternativeEndMinute = "00";
        m_strDuration = "30";
        m_intNumber = in_intNumber;
        m_strDescription = "keine";
    }

    public void setDescription( String in_strDescription )
    {
        m_strDescription = in_strDescription;
    }

    public String getDescription()
    {
        return m_strDescription;
    }

    public void setNumber( int in_intNumber )
    {
        m_intNumber = in_intNumber;
    }

    public int getNumber()
    {
        return m_intNumber;
    }

    public void setDuration( String in_strDuration )
    {
        m_strDuration = in_strDuration;
    }

    public String getDuration()
    {
        return m_strDuration;
    }

    public void setRegularStartHour( String in_strRegularStartHour )
    {
        m_strRegularStartHour = in_strRegularStartHour;
    }

    public String getRegularStartHour()
    {
        return m_strRegularStartHour;
    }

    public void setRegularStartMinute( String in_strRegularStartMinute )
    {
        m_strRegularStartMinute = in_strRegularStartMinute;
    }

    public String getRegularStartMinute()
    {
        return m_strRegularStartMinute;
    }

    public void setRegularEndHour( String in_strRegularEndHour )
    {
        m_strRegularEndHour = in_strRegularEndHour;
    }

    public String getRegularEndHour()
    {
        return m_strRegularEndHour;
    }

    public void setRegularEndMinute( String in_strRegularEndMinute )
    {
        m_strRegularEndMinute = in_strRegularEndMinute;
    }

    public String getRegularEndMinute()
    {
        return m_strRegularEndMinute;
    }

    public void setAlternativeStartHour( String in_strAlternativeStartHour )
    {
        m_strAlternativeStartHour = in_strAlternativeStartHour;
    }

    public String getAlternativeStartHour()
    {
        return m_strAlternativeStartHour;
    }

    public void setAlternativeStartMinute( String in_strAlternativeStartMinute )
    {
        m_strAlternativeStartMinute = in_strAlternativeStartMinute;
    }

    public String getAlternativeStartMinute()
    {
        return m_strAlternativeStartMinute;
    }

    public void setAlternativeEndHour( String in_strAlternativeEndHour )
    {
        m_strAlternativeEndHour = in_strAlternativeEndHour;
    }

    public String getAlternativeEndHour()
    {
        return m_strAlternativeEndHour;
    }

    public void setAlternativeEndMinute( String in_strAlternativeEndMinute )
    {
        m_strAlternativeEndMinute = in_strAlternativeEndMinute;
    }

    public String getAlternativeEndMinute()
    {
        return m_strAlternativeEndMinute;
    }
}