<?xml version="1.0"?>
<Bussimulation xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="xsd/busSimulation.xsd">
	<Strassennetz>
	
		<!--  set an Haltestellen -->
		<Haltestellen>
			<!-- Haltestellen Werte -->
			<Haltestelle id="">
				<Name></Name>
				<Kapazitaet></Kapazitaet>
				<Point xValue="" yValue=""/>
			</Haltestelle>
		</Haltestellen>
		
		<!--  set an Planquadraten -->
		<Planquadrate>
		    <PlanquadratStrasse id=""><!--  to count the Streets  -->
				<Strasse id="" art="">
		        	<GesamtKosten></GesamtKosten>
		            <KostenStartBisPunkt></KostenStartBisPunkt>
		            <KostenPunktBisZiel></KostenPunktBisZiel>
		            <Point xStart="" yStart="" xEnd="" yEnd=""/>
		            <Feldgroesse width="" height=""/>
		        </Strasse>
			</PlanquadratStrasse>
			<PlanquadratGesperrteStrasse id=""><!--  to count the barredWays  -->
				<GesperrteStrasse id="" art="">
		        	<GesamtKosten></GesamtKosten>
		            <KostenStartBisPunkt></KostenStartBisPunkt>
		            <KostenPunktBisZiel></KostenPunktBisZiel>
		            <Point xStart="" yStart="" xEnd="" yEnd=""/>
		            <Feldgroesse width="" height=""/>
		        </GesperrteStrasse>
			</PlanquadratGesperrteStrasse>
		</Planquadrate>
		
		<!--  set an Zielen -->
		<Ziele>
			<Ziel id=""><!--  ein Ziel -->
				<Name></Name>
				<Point xValue="" yValue=""/>
			</Ziel>
		</Ziele>
		
		<!--  set an Linien -->
		<Linien>
			<Linie id="">
				<Haltestellen><!--  set an Haltestellen -->
					<Haltestelle id=""/>
				</Haltestellen>
				<Teilstrecken><!--  set an Teilstrecken -->
					<Teilstrecke id="">
						<Name></Name>
						<StartPoint xValue="" yValue=""/>
						<EndPoint xValue="" yValue=""/>
						<Geschwindigkeit></Geschwindigkeit>
						<Breite></Breite>
						<Laenge></Laenge>
					</Teilstrecke>
				</Teilstrecken>
				<Color></Color>
				<MaxKapazitaetPassagiere></MaxKapazitaetPassagiere>
				<Wiederkehrzeit></Wiederkehrzeit>
			</Linie>
		</Linien>
		
		<!--  set an gesperrte Felder -->
		<GesperrteFelder>
			<GesperrtesFeld id="">
				<Name></Name>
				<Point xValue="" yValue=""/>
			</GesperrtesFeld>
		</GesperrteFelder>

	</Strassennetz>
	
	<!--  SimuKonfiguration  -->
	<SimuKonfiguration>
		<AktuelleAufloesung resWidth="" resHeight=""/>
		<GroessePlanquadrat></GroessePlanquadrat>
		<EinstellungenFrame width="" height=""/>
		<GroesseFeldelement></GroesseFeldelement>
		<BusGeschwindigkeit></BusGeschwindigkeit>
		<GehGeschwindigkeit></GehGeschwindigkeit>
	</SimuKonfiguration>
	
	<!--  SimuPanel  -->
	<SimuPanel>
		<GroesseEditor></GroesseEditor>
		<GroesseFelder></GroesseFelder>
		<AnzahlFelder></AnzahlFelder>
		<BackgroundImage url="" width="" height=""/>
		<Haltestellen>
			<Haltestelle id=""/>
		</Haltestellen>
		<BusPanels>
			<BusPanel>
				<Image url="" width="" height=""/>
				<Bus id=""><!--  Bus Object -->
					<BusLinie id="">
						<HaltestellenJ>
							<HaltestelleJ>
								<BusWarteschlangeQueue>
									<Name></Name>
									<DurchschnittsLaenge></DurchschnittsLaenge>
									<DurchschnittsWarteZeit></DurchschnittsWarteZeit>
									<WarteschlangenLimit></WarteschlangenLimit>
									<WarteschlangenStrategie></WarteschlangenStrategie>
								</BusWarteschlangeQueue>
								<PassagierWarteschlangeQueue>
									<Name></Name>
									<DurchschnittsLaenge></DurchschnittsLaenge>
									<DurchschnittsWarteZeit></DurchschnittsWarteZeit>
									<WarteschlangenLimit></WarteschlangenLimit>
									<WarteschlangenStrategie></WarteschlangenStrategie>
								</PassagierWarteschlangeQueue>
								<Point xValue="" yValue=""/>
								<BusEinheiten></BusEinheiten>
								<MaxKapazitaet></MaxKapazitaet>
							</HaltestelleJ>
						</HaltestellenJ>
						<MaxKapazitaet></MaxKapazitaet>
						<Teilstrecken><!--  set an Teilstrecken -->
							<Teilstrecke id="">
								<Name></Name>
								<StartPoint xValue="" yValue=""/>
								<EndPoint xValue="" yValue=""/>
								<Geschwindigkeit></Geschwindigkeit>
								<Breite></Breite>
								<Laenge></Laenge>
							</Teilstrecke>
						</Teilstrecken>
					</BusLinie>
					<HaltestelleJ>
						<BusWarteschlangeQueue>
							<Name></Name>
							<DurchschnittsLaenge></DurchschnittsLaenge>
							<DurchschnittsWarteZeit></DurchschnittsWarteZeit>
							<WarteschlangenLimit></WarteschlangenLimit>
							<WarteschlangenStrategie></WarteschlangenStrategie>
						</BusWarteschlangeQueue>
						<PassagierWarteschlangeQueue>
							<Name></Name>
							<DurchschnittsLaenge></DurchschnittsLaenge>
							<DurchschnittsWarteZeit></DurchschnittsWarteZeit>
							<WarteschlangenLimit></WarteschlangenLimit>
							<WarteschlangenStrategie></WarteschlangenStrategie>
						</PassagierWarteschlangeQueue>
						<Point xValue="" yValue=""/>
						<BusEinheiten></BusEinheiten>
						<MaxKapazitaet></MaxKapazitaet>
					</HaltestelleJ>
					<PassagierQueue>
						<Name></Name>
						<DurchschnittsLaenge></DurchschnittsLaenge>
						<DurchschnittsWarteZeit></DurchschnittsWarteZeit>
						<PassagierQueueLimit></PassagierQueueLimit>
						<PassagierQueueStrategie></PassagierQueueStrategie>
					</PassagierQueue>
					<MaxKapazitaet></MaxKapazitaet>
					<Position xValue="" yValue=""/>
				</Bus>
			</BusPanel>
		</BusPanels>
		<BenoetigteBusse>
			<Bus id=""/>
		</BenoetigteBusse>
	</SimuPanel>
	
	<!--  VirtualGrid  -->
	<VirtualGrid>
		<!--  gets Data from SimuKonfiguration -->
	</VirtualGrid>
	
</Bussimulation>
